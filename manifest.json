{
  "manifest_version": 3,
  "name": "Bold First Letters",
  "version": "1.0",
  "description": "Select text and make the first letter of each word bold",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "3Bold.html",
    "default_icon": "extension.png"
  }
}
